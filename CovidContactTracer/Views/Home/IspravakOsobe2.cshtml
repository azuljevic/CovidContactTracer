                </div>

                <div class="col-8">
                <div class="row ">
                    <div class="col-6">
                        
                        <form asp-controller="home" asp-action="IspravakOsobe3" method="post">

                            <div asp-validation-summary="ModelOnly" class="text-danger"></div>
                            <div class="form-group">


                                <label asp-for="id" class="control-label">OIB</label>
                                <input asp-for="id" id="id" name="id" class="form-control" readonly />

                                <span asp-validation-for="id" class="text-danger"></span>
                            </div>
                            <div class="form-group">
                                <label asp-for="ime" class="control-label"></label>
                                <input asp-for="ime" class="form-control" required autocomplete="off" />
                                <span asp-validation-for="ime" class="text-danger"></span>
                            </div>
                            <div class="form-group">
                                <label asp-for="rezultat" class="control-label"></label>
                                <select asp-for="rezultat" class="form-control" required>
                                    <option>Pozitivan</option>
                                    <option>Negativan</option>
                                </select>
                                <span asp-validation-for="rezultat" class="text-danger"></span>
                            </div>

                            <div class="form-group">
                                <label asp-for="vrijemetestiranja" class="control-label"></label>
                                <input asp-for="vrijemetestiranja" class="form-control" required />
                                <span asp-validation-for="vrijemetestiranja" class="text-danger"></span>
                            </div>
                            <div class="form-group ">

                                <input type="submit" value="Spremi" class="btn btn-primary" />
                            </div>

                            @*<div class="form-group">
            <input type="submit" value="Izbrisi " class="btn btn-danger" />
        </div>*@
                            <button class="btn btn-danger" type="submit" formaction=@Url.Action("ObrisiOsobu") formmethod="post">Izbriši</button>

                        </form>

                    </div>
                </div>
                    </div>

                

            </div>
